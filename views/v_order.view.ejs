<%- include('parts/header') %> 
<%- include('parts/navbar') %>
<div class="container">
    <p>
        <h3>
            Verify your order
        </h3>
    </p>
    <%= DocId.name %> 
    <form action="/order" method="POST">
        <div class="input-group input-group-lg">
            <input type="text" name="address" class="form-control" placeholder="Shipping adress">
            <input type="hidden" name="item" value="<%= DocId._id %> ">
            <input type="hidden" name="name" value="<%= DocId.name %> ">
            <input type="hidden" name="qty" value="<%= DocId.qty %> ">
            <input type="hidden" name="price" value="<%= DocId.price %> ">
        </div>
            <br>         
        <input type="submit" value="Order" class="btn btn-primary">
    </form>

</div> 
<%- include('parts/footer') %> 